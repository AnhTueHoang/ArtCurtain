<h1> New Record</h1>
<form action="/customer" method="post">
<table>
    <tr> <td> First Name: </td> <td> <input type="text" name="firstname" required> </td> </tr>
    <tr> <td> Last Name: </td> <td> <input type="text" name="lastname" required> </td> </tr>
    <tr> <td> Email: </td> <td> <textarea name="email"
    placeholder="Email" required></textarea> </td> </tr>
    <tr> <td> Phone: </td> <td> <input type="number" name="phone"
    placeholder="Phone"></input> </td> </tr>
    <tr> <td> Address 1: </td> <td> <input type="text" name="address1" required></input> </td> </tr>
    <tr> <td> Address 2: </td> <td> <textarea name="address2"></textarea> </td> </tr>
    <tr> <td> City: </td> <td> <input type="text" name="city" required> </td> </tr>
    <tr> <td> State: </td> <td> <input type="text" name="state" required> </td> </tr>
    <tr> <td> Zip: </td> <td> <input type="number" name="zip" required> </td> </tr>
    <tr> <td> Username: </td> <td> <input name="username" required>
    <tr> <td> Password: </td> <td> <input type="password" name="password" required>
</table>
<button type="submit">Save</button>
